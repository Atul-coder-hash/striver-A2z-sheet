#include<bits/stdc++.h>
using namespace std;

int second_large(int arr[],int size)
{
    int large = INT_MIN;
    int second_large = INT_MIN;
    
    for(int i=0;i<size;i++)
    {
    large = max(large,arr[i]);
    }
    for(int i=0;i<size;i++)
    {
        if(arr[i]>second_large&&arr[i]!=large)
        {
            second_large = arr[i];
        }
    }
    
    return second_large;
}

int main(){
    
   int a[6]={1,2,4,6,7,5};
  
  cout<<second_large(a,6);

}
