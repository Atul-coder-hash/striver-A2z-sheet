#include<bits/stdc++.h>
using namespace std;

int main()
{
    string s ="abcdeabc";
    int hash[26]={0};
    
    
    for(int i=0;i<s.size();i++)
    {
        hash[s[i]-'a']++;
        
    }
    
    int q;
    cout<<"enter query:-";
    cin>>q;
    while(q--)
    {char ch;
    cout<<"enter character:-";
    cin>>ch;
    cout<<"counting of character "<<ch<<" in string is:-"<<hash[ch-'a']<<endl;
    }
    
    
    
}
