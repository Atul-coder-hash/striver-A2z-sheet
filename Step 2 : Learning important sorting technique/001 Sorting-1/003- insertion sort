#include<bits/stdc++.h>
using namespace std;
int main()
{int n=7;
int a[n]={13,46,24,52,20,25,10};

for(int i=0;i<n;i++)
{
    for(int j=i;j>0;j--)
    {
        if(a[j-1]>a[j])
        {
            swap(a[j],a[j-1]);// swap with adjacent  element
       
        }
        
    }
}


for(int i=0;i<n;i++)
{
    cout<<a[i]<<" ";
}

}
