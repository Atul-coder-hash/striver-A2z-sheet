#include<bits/stdc++.h>
using namespace std;
int main()
{int n=6;
int a[6]={13,46,24,52,20,9};

for(int i=0;i<n;i++)
{
    for(int j=0;j<n-i-1;j++)
    {
        if(a[j]>a[j+1])
        {
            swap(a[j],a[j+1]);// swap with adjacent  element
       
        }
        
    }
}


for(int i=0;i<n;i++)
{
    cout<<a[i]<<" ";
}

}
