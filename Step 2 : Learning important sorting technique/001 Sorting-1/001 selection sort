#include<bits/stdc++.h>
using namespace std;
int main()
{int n=6;
int a[6]={13,46,24,52,90,9};

for(int i=0;i<n;i++)
{
    int min=i;
    for(int j=i+1;j<n;j++)  // find minimum and update minimum
    {
        if(a[j]<a[min])
        {
            min=j;
        }
        
    }
    swap(a[i],a[min]);// swap with first element
}

for(int i=0;i<n;i++)
{
    cout<<a[i]<<" ";
}
}
